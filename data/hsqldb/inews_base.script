INSERT INTO NEWS VALUES(1,'2013-05-16 01:14:20.000000000','images/news1/Medvedev-1.jpg','images/news1/scaledMedvedev-1.jpg','\u041f\u0440\u0435\u043c\u044c\u0435\u0440-\u043c\u0438\u043d\u0438\u0441\u0442\u0440 \u0420\u043e\u0441\u0441\u0438\u0438 \u0414\u043c\u0438\u0442\u0440\u0438\u0439 \u041c\u0435\u0434\u0432\u0435\u0434\u0435\u0432 \u0441\u0442\u0430\u043b \u0434\u0435\u043b\u0430\u0442\u044c \u043f\u043e\u043c\u0435\u0442\u043a\u0438 \u043d\u0435 \u0432 iPad, \u0430 \u0432 \u0431\u043b\u043e\u043a\u043d\u043e\u0442\u0435, \u0447\u0442\u043e\u0431\u044b \u043d\u0438\u043a\u0442\u043e \u043d\u0435 \u043f\u043e\u0434\u0443\u043c\u0430\u043b, \u0447\u0442\u043e \u043e\u043d \u0438\u0433\u0440\u0430\u0435\u0442. \u041e\u0431 \u044d\u0442\u043e\u043c \u043e\u043d \u0440\u0430\u0441\u0441\u043a\u0430\u0437\u0430\u043b \u043d\u0430 \u041c\u0435\u0436\u0434\u0443\u043d\u0430\u0440\u043e\u0434\u043d\u043e\u043c \u044e\u0440\u0438\u0434\u0438\u0447\u0435\u0441\u043a\u043e\u043c \u0444\u043e\u0440\u0443\u043c\u0435 \u0432 \u0421\u0430\u043d\u043a\u0442-\u041f\u0435\u0442\u0435\u0440\u0431\u0443\u0440\u0433\u0435, \u0441\u043e\u043e\u0431\u0449\u0430\u0435\u0442 \u0420\u0418\u0410 \u041d\u043e\u0432\u043e\u0441\u0442\u0438.\u000d\u000a\u00ab\u041f\u0438\u0441\u0430\u043b \u043e\u0442 \u0440\u0443\u043a\u0438, \u0447\u0442\u043e\u0431\u044b \u043d\u0435 \u0431\u044b\u043b\u043e \u043d\u0438\u043a\u0430\u043a\u0438\u0445 \u0441\u043e\u043c\u043d\u0435\u043d\u0438\u0439, \u0447\u0442\u043e \u044f \u0441\u043b\u0443\u0448\u0430\u043b \u0441\u0432\u043e\u0438\u0445 \u043a\u043e\u043b\u043b\u0435\u0433\u00bb, \u2014 \u0441\u043a\u0430\u0437\u0430\u043b \u043f\u0440\u0435\u043c\u044c\u0435\u0440. \u041e\u043d \u0434\u043e\u0431\u0430\u0432\u0438\u043b, \u0447\u0442\u043e \u043d\u0430 \u043f\u0440\u0435\u0434\u044b\u0434\u0443\u0449\u0435\u043c \u0437\u0430\u0441\u0435\u0434\u0430\u043d\u0438\u0438 \u0444\u043e\u0440\u0443\u043c\u0430 \u0438\u0441\u043f\u043e\u043b\u044c\u0437\u043e\u0432\u0430\u043b \u0434\u043b\u044f \u0437\u0430\u043c\u0435\u0442\u043e\u043a iPad, \u043f\u043e\u0441\u043b\u0435 \u0447\u0435\u0433\u043e \u0435\u0433\u043e \u0443\u043f\u0440\u0435\u043a\u043d\u0443\u043b\u0438, \u0447\u0442\u043e \u043e\u043d \u00ab\u0438\u0433\u0440\u0430\u043b \u0432\u0435\u0441\u044c \u0444\u043e\u0440\u0443\u043c\u00bb.\u000d\u000a\u000d\u000a\u0414\u043c\u0438\u0442\u0440\u0438\u0439 \u041c\u0435\u0434\u0432\u0435\u0434\u0435\u0432 \u0438\u0437\u0432\u0435\u0441\u0442\u0435\u043d \u0441\u0432\u043e\u0435\u0439 \u043b\u044e\u0431\u043e\u0432\u044c\u044e \u043a \u0443\u0441\u0442\u0440\u043e\u0439\u0441\u0442\u0432\u0430\u043c Apple. \u0412\u043f\u0435\u0440\u0432\u044b\u0435 \u043f\u043b\u0430\u043d\u0448\u0435\u0442 iPad \u043f\u043e\u044f\u0432\u0438\u043b\u0441\u044f \u0443 \u043d\u0435\u0433\u043e \u0432\u0435\u0441\u043d\u043e\u0439 2010 \u0433\u043e\u0434\u0430. \u0422\u043e\u0433\u0434\u0430 \u041c\u0435\u0434\u0432\u0435\u0434\u0435\u0432, \u0431\u044b\u0432\u0448\u0438\u0439 \u043f\u0440\u0435\u0437\u0438\u0434\u0435\u043d\u0442\u043e\u043c \u0420\u043e\u0441\u0441\u0438\u0438, \u0437\u0430\u044f\u0432\u0438\u043b, \u0447\u0442\u043e \u0442\u0435, \u043a\u0442\u043e \u043f\u044b\u0442\u0430\u0435\u0442\u0441\u044f \u0437\u0430\u043a\u0440\u044b\u0442\u044c\u0441\u044f \u043e\u0442 \u0442\u0435\u0445\u043d\u043e\u043b\u043e\u0433\u0438\u0439, \u0441\u043e\u0432\u0435\u0440\u0448\u0430\u044e\u0442 \u043e\u0448\u0438\u0431\u043a\u0443 \u0438 \u043b\u0438\u0448\u0430\u044e\u0442 \u0441\u0435\u0431\u044f \u0443\u0434\u043e\u0432\u043e\u043b\u044c\u0441\u0442\u0432\u0438\u044f. \u0412 \u0442\u043e\u043c \u0436\u0435 \u0433\u043e\u0434\u0443 \u043e\u0441\u043d\u043e\u0432\u0430\u0442\u0435\u043b\u044c Apple \u0421\u0442\u0438\u0432 \u0414\u0436\u043e\u0431\u0441 \u043b\u0438\u0447\u043d\u043e \u043f\u043e\u0434\u0430\u0440\u0438\u043b \u0435\u043c\u0443 \u0441\u043c\u0430\u0440\u0442\u0444\u043e\u043d iPhone 4 \u0437\u0430 \u0434\u0435\u043d\u044c \u0434\u043e \u043e\u0444\u0438\u0446\u0438\u0430\u043b\u044c\u043d\u043e\u0439 \u043f\u0440\u0435\u043c\u044c\u0435\u0440\u044b \u0442\u0435\u043b\u0435\u0444\u043e\u043d\u0430 \u0432 \u0421\u0428\u0410. \u0412 2011 \u0433\u043e\u0434\u0443 \u0440\u0435\u0437\u0438\u0434\u0435\u043d\u0442\u044b \u043f\u0440\u043e\u0435\u043a\u0442\u0430 Comedy Club \u043f\u043e\u0434\u0430\u0440\u0438\u043b\u0438 \u041c\u0435\u0434\u0432\u0435\u0434\u0435\u0432\u0443 \u043f\u043b\u0430\u043d\u0448\u0435\u0442 iPad 2 \u0441\u043e \u0432\u0441\u0435\u043c\u0438 \u0432\u044b\u043f\u0443\u0441\u043a\u0430\u043c\u0438 \u043a\u043e\u043c\u0435\u0434\u0438\u0439\u043d\u043e\u0433\u043e \u0448\u043e\u0443.\u000d\u000a\u000d\u000a\u0412\u0441\u043b\u0435\u0434 \u0437\u0430 \u041c\u0435\u0434\u0432\u0435\u0434\u0435\u0432\u044b\u043c \u043f\u043b\u0430\u043d\u0448\u0435\u0442\u0430\u043c\u0438 iPad \u0441\u0442\u0430\u043b\u0438 \u043f\u043e\u043b\u044c\u0437\u043e\u0432\u0430\u0442\u044c\u0441\u044f \u0440\u043e\u0441\u0441\u0438\u0439\u0441\u043a\u0438\u0435 \u0447\u0438\u043d\u043e\u0432\u043d\u0438\u043a\u0438, \u0432 \u0442\u043e\u043c \u0447\u0438\u0441\u043b\u0435 \u0447\u043b\u0435\u043d\u044b \u043f\u0440\u0430\u0432\u0438\u0442\u0435\u043b\u044c\u0441\u0442\u0432\u0430. \u0412 \u0447\u0430\u0441\u0442\u043d\u043e\u0441\u0442\u0438, \u043a\u0430\u043a \u0441\u043e\u043e\u0431\u0449\u0430\u043b\u0438 \u0421\u041c\u0418, \u0442\u0430\u043a\u043e\u0439 \u043f\u043b\u0430\u043d\u0448\u0435\u0442 \u043f\u043e\u043b\u0443\u0447\u0438\u043b \u043a\u0430\u0436\u0434\u044b\u0439 \u0434\u0435\u043f\u0443\u0442\u0430\u0442 \u0413\u043e\u0441\u0434\u0443\u043c\u044b \u0448\u0435\u0441\u0442\u043e\u0433\u043e \u0441\u043e\u0437\u044b\u0432\u0430. \u0412\u043f\u043e\u0441\u043b\u0435\u0434\u0441\u0442\u0432\u0438\u0438 \u043f\u043e\u044f\u0432\u043b\u044f\u043b\u0438\u0441\u044c \u0441\u043e\u043e\u0431\u0449\u0435\u043d\u0438\u044f, \u0447\u0442\u043e \u043f\u043b\u0430\u043d\u0448\u0435\u0442\u044b iPad \u0437\u0430\u043a\u0443\u043f\u0430\u044e\u0442 \u0432 \u0440\u0435\u0433\u0438\u043e\u043d\u0430\u043b\u044c\u043d\u044b\u0445 \u043f\u0430\u0440\u043b\u0430\u043c\u0435\u043d\u0442\u0430\u0445.\u000d\u000a\u000d\u000a\u041f\u0440\u0438\u0441\u043e\u0435\u0434\u0438\u043d\u044f\u0439\u0442\u0435\u0441\u044c \u043a \u043d\u0430\u043c \u0432 Twitter, \u0412\u041a\u043e\u043d\u0442\u0430\u043a\u0442\u0435, Facebook, Google+ \u0438\u043b\u0438 \u0447\u0435\u0440\u0435\u0437 RSS, \u0447\u0442\u043e\u0431\u044b \u0431\u044b\u0442\u044c \u0432 \u043a\u0443\u0440\u0441\u0435 \u043f\u043e\u0441\u043b\u0435\u0434\u043d\u0438\u0445 \u043d\u043e\u0432\u043e\u0441\u0442\u0435\u0439 \u0438\u0437 \u043c\u0438\u0440\u0430 Apple, Microsoft \u0438 Google.','\u041c\u0435\u0434\u0432\u0435\u0434\u0435\u0432 \u043e\u0431\u044a\u044f\u0441\u043d\u0438\u043b, \u043f\u043e\u0447\u0435\u043c\u0443 \u043e\u043d \u043e\u0442\u043a\u0430\u0437\u0430\u043b\u0441\u044f \u043e\u0442 iPad')
INSERT INTO NEWS VALUES(2,'2013-05-16 01:17:59.000000000','images/news2/my-peace-2.jpg','images/news2/scaledmy-peace-2.jpg','\u0421\u043e\u0446\u0438\u0430\u043b\u044c\u043d\u0430\u044f \u0441\u0435\u0442\u044c \u041c\u043e\u0439 \u041c\u0438\u0440 \u0432\u044b\u043f\u0443\u0441\u0442\u0438\u043b\u0430 \u043e\u0444\u0438\u0446\u0438\u0430\u043b\u044c\u043d\u043e\u0435 \u043c\u043e\u0431\u0438\u043b\u044c\u043d\u043e\u0435 \u043f\u0440\u0438\u043b\u043e\u0436\u0435\u043d\u0438\u0435 \u0434\u043b\u044f \u0432\u043b\u0430\u0434\u0435\u043b\u044c\u0446\u0435\u0432 iPhone. \u0421 \u043f\u043e\u043c\u043e\u0449\u044c\u044e \u043f\u0440\u043e\u0433\u0440\u0430\u043c\u043c\u044b \u0443\u0434\u043e\u0431\u043d\u043e \u0434\u0435\u043b\u0438\u0442\u044c\u0441\u044f \u000d\u000a\u0444\u043e\u0442\u043e\u0433\u0440\u0430\u0444\u0438\u044f\u043c\u0438 \u0438 \u043d\u043e\u0432\u043e\u0441\u0442\u044f\u043c\u0438, \u0437\u0430\u0432\u043e\u0434\u0438\u0442\u044c \u043d\u043e\u0432\u044b\u0445 \u0434\u0440\u0443\u0437\u0435\u0439 \u0438 \u043e\u0431\u0449\u0430\u0442\u044c\u0441\u044f \u0441\u043e \u0441\u0442\u0430\u0440\u044b\u043c\u0438.\u000d\u000a\u000d\u000a\u000d\u000a\u000d\u000a\u041e\u0441\u043e\u0431\u043e\u0435 \u0432\u043d\u0438\u043c\u0430\u043d\u0438\u0435 \u0440\u0430\u0437\u0440\u0430\u0431\u043e\u0442\u0447\u0438\u043a\u0438 \u0443\u0434\u0435\u043b\u0438\u043b\u0438 \u0440\u0430\u0431\u043e\u0442\u0435 \u0441\u043e \u0441\u043d\u0438\u043c\u043a\u0430\u043c\u0438. \u0414\u043b\u044f \u0442\u043e\u0433\u043e \u0447\u0442\u043e\u0431\u044b \u0432\u044b\u043b\u043e\u0436\u0438\u0442\u044c \u0443\u0434\u0430\u0447\u043d\u043e\u0435 \u0444\u043e\u0442\u043e, \u0434\u043e\u0441\u0442\u0430\u0442\u043e\u0447\u043d\u043e \u043f\u0430\u0440\u044b \u043d\u0430\u0436\u0430\u0442\u0438\u0439. \u041f\u043e\u043b\u044c\u0437\u043e\u0432\u0430\u0442\u0435\u043b\u044c \u043c\u043e\u0436\u0435\u0442, \u000d\u000a\u043d\u0435 \u043f\u043e\u043a\u0438\u0434\u0430\u044f \u043f\u0440\u0438\u043b\u043e\u0436\u0435\u043d\u0438\u044f, \u0441\u0434\u0435\u043b\u0430\u0442\u044c \u0441\u043d\u0438\u043c\u043e\u043a, \u043e\u0431\u0440\u0430\u0431\u043e\u0442\u0430\u0442\u044c \u0435\u0433\u043e \u0441 \u043f\u043e\u043c\u043e\u0449\u044c\u044e \u0440\u0435\u0434\u0430\u043a\u0442\u043e\u0440\u0430 \u0424\u043e\u0442\u043e\u043c\u0430\u0433\u0438\u044f \u0438 \u043e\u043f\u0443\u0431\u043b\u0438\u043a\u043e\u0432\u0430\u0442\u044c \u0432 \u041c\u043e\u0435\u043c \u041c\u0438\u0440\u0435. \u041f\u0440\u043e\u0441\u043c\u043e\u0442\u0440 \u0444\u043e\u0442\u043e, \u000d\u000a\u043e\u043f\u0443\u0431\u043b\u0438\u043a\u043e\u0432\u0430\u043d\u043d\u044b\u0445 \u0434\u0440\u0443\u0437\u044c\u044f\u043c\u0438, \u0442\u043e\u0436\u0435 \u0430\u0434\u0430\u043f\u0442\u0438\u0440\u043e\u0432\u0430\u043b\u0438 \u0434\u043b\u044f \u0441\u043c\u0430\u0440\u0442\u0444\u043e\u043d\u043e\u0432: \u043b\u0438\u0441\u0442\u0430\u0442\u044c \u0430\u043b\u044c\u0431\u043e\u043c\u044b \u0432 \u0424\u043e\u0442\u043e\u043f\u043e\u0442\u043e\u043a\u0435 \u043c\u043e\u0436\u043d\u043e \u043f\u0440\u0438\u0432\u044b\u0447\u043d\u044b\u043c \u0436\u0435\u0441\u0442\u043e\u043c \u0441\u0432\u0430\u0439\u043f.\u000d\u000a\u000d\u000a\u000d\u000a\u000d\u000a\u00ab\u0414\u043b\u044f \u0441\u043e\u0446\u0438\u0430\u043b\u044c\u043d\u044b\u0445 \u0441\u0435\u0442\u0435\u0439 \u043e\u0447\u0435\u043d\u044c \u0432\u0430\u0436\u043d\u0430 \u0432\u043e\u0437\u043c\u043e\u0436\u043d\u043e\u0441\u0442\u044c \u0434\u0435\u043b\u0438\u0442\u044c\u0441\u044f \u044d\u043c\u043e\u0446\u0438\u044f\u043c\u0438, \u0443\u0434\u0430\u0447\u043d\u044b\u043c\u0438 \u043a\u0430\u0434\u0440\u0430\u043c\u0438 \u0438 \u043a\u043e\u043c\u043c\u0435\u043d\u0442\u0438\u0440\u043e\u0432\u0430\u0442\u044c \u0442\u0430\u043a\u0438\u0435 \u043c\u043e\u043c\u0435\u043d\u0442\u044b \u0432 \u043b\u0435\u043d\u0442\u0435 \u0434\u0440\u0443\u0437\u0435\u0439. \u0421\u043c\u0430\u0440\u0442\u0444\u043e\u043d\u044b \u000d\u000a\u0434\u043b\u044f \u0432\u0441\u0435\u0433\u043e \u044d\u0442\u043e\u0433\u043e \u043f\u043e\u0434\u0445\u043e\u0434\u044f\u0442 \u043a\u0430\u043a \u043d\u0435\u043b\u044c\u0437\u044f \u043b\u0443\u0447\u0448\u0435. \u0418\u043c\u0435\u043d\u043d\u043e \u043f\u043e\u044d\u0442\u043e\u043c\u0443 \u0440\u0430\u0437\u0440\u0430\u0431\u043e\u0442\u043a\u0430 \u0444\u0443\u043d\u043a\u0446\u0438\u043e\u043d\u0430\u043b\u044c\u043d\u044b\u0445 \u0438\u043d\u0441\u0442\u0440\u0443\u043c\u0435\u043d\u0442\u043e\u0432 \u043c\u043e\u0431\u0438\u043b\u044c\u043d\u043e\u0433\u043e \u0434\u043e\u0441\u0442\u0443\u043f\u0430 \u0432 \u041c\u043e\u0439 \u041c\u0438\u0440 \u044f\u0432\u043b\u044f\u0435\u0442\u0441\u044f \u000d\u000a\u043e\u0434\u043d\u043e\u0439 \u0438\u0437 \u0432\u0430\u0436\u043d\u0435\u0439\u0448\u0438\u0445 \u043d\u0430\u0448\u0438\u0445 \u0437\u0430\u0434\u0430\u0447 \u043d\u0430 \u0431\u043b\u0438\u0436\u0430\u0439\u0448\u0435\u0435 \u0431\u0443\u0434\u0443\u0449\u0435\u0435\u00bb, \u2014 \u043a\u043e\u043c\u043c\u0435\u043d\u0442\u0438\u0440\u0443\u0435\u0442 \u0440\u0443\u043a\u043e\u0432\u043e\u0434\u0438\u0442\u0435\u043b\u044c \u0441\u043e\u0446\u0438\u0430\u043b\u044c\u043d\u043e\u0439 \u0441\u0435\u0442\u0438 \u041c\u043e\u0439 \u041c\u0438\u0440 \u0414\u0435\u043d\u0438\u0441 \u0410\u043b\u0430\u0435\u0432.\u000d\u000a\u000d\u000a\u0421 \u043f\u043e\u043c\u043e\u0449\u044c\u044e \u043f\u0440\u0438\u043b\u043e\u0436\u0435\u043d\u0438\u044f \u043f\u043e\u043b\u044c\u0437\u043e\u0432\u0430\u0442\u0435\u043b\u0438 \u043c\u043e\u0433\u0443\u0442 \u0441\u043b\u0443\u0448\u0430\u0442\u044c \u043c\u0443\u0437\u044b\u043a\u0443, \u0440\u0430\u0441\u0441\u043a\u0430\u0437\u044b\u0432\u0430\u0442\u044c \u043e \u0442\u043e\u043c, \u0447\u0442\u043e \u043f\u0440\u043e\u0438\u0441\u0445\u043e\u0434\u0438\u0442 \u0443 \u043d\u0438\u0445, \u0447\u0438\u0442\u0430\u0442\u044c \u0438 \u043a\u043e\u043c\u043c\u0435\u043d\u0442\u0438\u0440\u043e\u0432\u0430\u0442\u044c \u043d\u043e\u0432\u043e\u0441\u0442\u0438 \u0434\u0440\u0443\u0437\u0435\u0439, \u000d\u000a\u043e\u0431\u0449\u0430\u0442\u044c\u0441\u044f, \u043f\u0440\u0438\u043d\u0438\u043c\u0430\u0442\u044c \u043f\u0440\u0435\u0434\u043b\u043e\u0436\u0435\u043d\u0438\u044f \u0434\u0440\u0443\u0436\u0431\u044b \u0438 \u0441\u043c\u043e\u0442\u0440\u0435\u0442\u044c, \u043a\u0442\u043e \u0437\u0430\u0445\u043e\u0434\u0438\u043b \u043d\u0430 \u0441\u0442\u0440\u0430\u043d\u0438\u0447\u043a\u0443. \u0414\u043b\u044f \u0431\u044b\u0441\u0442\u0440\u043e\u0433\u043e \u0438 \u0443\u0434\u043e\u0431\u043d\u043e\u0433\u043e \u043e\u0431\u0449\u0435\u043d\u0438\u044f \u0441 \u0434\u0440\u0443\u0437\u044c\u044f\u043c\u0438 \u0432 \u041c\u043e\u0435\u043c \u041c\u0438\u0440\u0435 \u000d\u000a\u0444\u0443\u043d\u043a\u0446\u0438\u043e\u043d\u0438\u0440\u0443\u044e\u0442 \u043d\u0430\u0442\u0438\u0432\u043d\u044b\u0435 \u0434\u0438\u0430\u043b\u043e\u0433\u0438.\u000d\u000a\u000d\u000a\u000d\u000a\u000d\u000a\u041f\u0440\u043e\u0447\u0438\u0435 \u0444\u0443\u043d\u043a\u0446\u0438\u0438 \u043c\u043e\u0431\u0438\u043b\u044c\u043d\u043e\u0433\u043e \u043f\u0440\u0438\u043b\u043e\u0436\u0435\u043d\u0438\u044f \u043f\u0440\u0430\u043a\u0442\u0438\u0447\u0435\u0441\u043a\u0438 \u0438\u0434\u0435\u043d\u0442\u0438\u0447\u043d\u044b \u0432\u043e\u0437\u043c\u043e\u0436\u043d\u043e\u0441\u0442\u044f\u043c \u0432\u0435\u0431-\u0432\u0435\u0440\u0441\u0438\u0438 \u041c\u043e\u0435\u0433\u043e \u041c\u0438\u0440\u0430. \u0417\u0434\u0435\u0441\u044c \u0432\u0441\u0435 \u043e\u0440\u0433\u0430\u043d\u0438\u0437\u043e\u0432\u0430\u043d\u043e \u0440\u0430\u0446\u0438\u043e\u043d\u0430\u043b\u044c\u043d\u043e, \u043a\u0430\u043a \u000d\u000a\u0442\u043e\u0433\u043e \u0442\u0440\u0435\u0431\u0443\u044e\u0442 \u043d\u0435\u0431\u043e\u043b\u044c\u0448\u0438\u0435 \u044d\u043a\u0440\u0430\u043d\u044b \u043c\u043e\u0431\u0438\u043b\u044c\u043d\u044b\u0445 \u0443\u0441\u0442\u0440\u043e\u0439\u0441\u0442\u0432, \u043f\u0440\u0438 \u044d\u0442\u043e\u043c \u0441\u043e\u0445\u0440\u0430\u043d\u0435\u043d\u0430 \u043f\u0440\u0435\u0435\u043c\u0441\u0442\u0432\u0435\u043d\u043d\u043e\u0441\u0442\u044c, \u0447\u0442\u043e \u043f\u043e\u043c\u043e\u0433\u0430\u0435\u0442 \u043f\u043e\u043b\u044c\u0437\u043e\u0432\u0430\u0442\u0435\u043b\u044f\u043c \u043b\u0435\u0433\u043a\u043e \u043e\u0440\u0438\u0435\u043d\u0442\u0438\u0440\u043e\u0432\u0430\u0442\u044c\u0441\u044f \u0432 \u000d\u000a\u043e\u0431\u0435\u0438\u0445 \u0432\u0435\u0440\u0441\u0438\u044f\u0445.\u000d\u000a\u000d\u000a\u0421\u043a\u0430\u0447\u0430\u0442\u044c \u041c\u043e\u0439 \u041c\u0438\u0440 \u0434\u043b\u044f iPhone \u0438 iPod touch \u043c\u043e\u0436\u043d\u043e \u0432 App Store \u043f\u043e \u044d\u0442\u043e\u0439 \u0441\u0441\u044b\u043b\u043a\u0435.\u000d\u000a\u000d\u000a\u041f\u0440\u0438\u0441\u043e\u0435\u0434\u0438\u043d\u044f\u0439\u0442\u0435\u0441\u044c \u043a \u043d\u0430\u043c \u0432 Twitter, \u0412\u041a\u043e\u043d\u0442\u0430\u043a\u0442\u0435, Facebook, Google+ \u0438\u043b\u0438 \u0447\u0435\u0440\u0435\u0437 RSS, \u0447\u0442\u043e\u0431\u044b \u0431\u044b\u0442\u044c \u0432 \u043a\u0443\u0440\u0441\u0435 \u043f\u043e\u0441\u043b\u0435\u0434\u043d\u0438\u0445 \u043d\u043e\u0432\u043e\u0441\u0442\u0435\u0439 \u0438\u0437 \u043c\u0438\u0440\u0430 Apple, Microsoft \u0438 \u000d\u000aGoogle.','\u041e\u0444\u0438\u0446\u0438\u0430\u043b\u044c\u043d\u043e\u0435 \u043f\u0440\u0438\u043b\u043e\u0436\u0435\u043d\u0438\u0435 \u041c\u043e\u0439 \u041c\u0438\u0440 \u0432\u044b\u0448\u043b\u043e \u0432 App Store')
INSERT INTO NEWS VALUES(3,'2013-05-16 01:19:19.000000000','images/news3/iPhone-5v-vs-all-2.jpg','images/news3/scalediPhone-5v-vs-all-2.jpg','\u0411\u0443\u0440\u043d\u043e\u0435 \u0440\u0430\u0437\u0432\u0438\u0442\u0438\u0435 \u0442\u0435\u0445\u043d\u043e\u043b\u043e\u0433\u0438\u0439 \u0432 \u043e\u0431\u043b\u0430\u0441\u0442\u0438 \u0444\u043e\u0442\u043e- \u0438 \u0432\u0438\u0434\u0435\u043e\u0441\u044a\u0435\u043c\u043a\u0438, \u043e\u0441\u043e\u0431\u0435\u043d\u043d\u043e \u043f\u0440\u043e\u0446\u0435\u0441\u0441\u043e\u0440\u043d\u043e\u0439 \u043f\u043e\u0441\u0442\u043e\u0431\u0440\u0430\u0431\u043e\u0442\u043a\u0438, \u043f\u0440\u0438\u0432\u0435\u043b\u043e \u043a \u0442\u043e\u043c\u0443, \u0447\u0442\u043e \u043f\u0440\u043e\u0438\u0437\u0432\u043e\u0434\u0438\u0442\u0435\u043b\u0438 \u043a\u0430\u043c\u0435\u0440 \u000d\u000a\u0432\u0441\u0435\u0440\u044c\u0435\u0437 \u043e\u0431\u0435\u0441\u043f\u043e\u043a\u043e\u0435\u043d\u044b \u043d\u0430\u0440\u0430\u0441\u0442\u0430\u044e\u0449\u0435\u0439 \u043a\u043e\u043d\u043a\u0443\u0440\u0435\u043d\u0446\u0438\u0435\u0439 \u0441\u043e \u0441\u0442\u043e\u0440\u043e\u043d\u044b \u0441\u043c\u0430\u0440\u0442\u0444\u043e\u043d\u043e\u0432 \u2014 \u0441\u043b\u0438\u0448\u043a\u043e\u043c \u043c\u043d\u043e\u0433\u0438\u0435 \u0432\u043b\u0430\u0434\u0435\u043b\u044c\u0446\u044b \u044d\u0442\u0438\u0445 \u0433\u0430\u0434\u0436\u0435\u0442\u043e\u0432 \u043d\u0435 \u0432\u0438\u0434\u044f\u0442 \u043d\u0435\u043e\u0431\u0445\u043e\u0434\u0438\u043c\u043e\u0441\u0442\u0438 \u0432 \u000d\u000a\u043f\u043e\u043a\u0443\u043f\u043a\u0435 \u043e\u0442\u0434\u0435\u043b\u044c\u043d\u043e\u0433\u043e \u0444\u043e\u0442\u043e\u0430\u043f\u043f\u0430\u0440\u0430\u0442\u0430. \u0418\u0437\u0434\u0430\u043d\u0438\u0435 Cnews, \u043f\u0440\u043e\u0442\u0435\u0441\u0442\u0438\u0440\u043e\u0432\u0430\u0432 \u0447\u0435\u0442\u044b\u0440\u0435 \u0444\u043b\u0430\u0433\u043c\u0430\u043d\u0441\u043a\u0438\u0445 \u0442\u0435\u043b\u0435\u0444\u043e\u043d\u0430 \u2013 iPhone 5, Nokia Lumia 920, Sony Xperia Z \u0438 HTC One, \u000d\u000a\u043f\u0440\u0438\u0448\u043b\u043e \u043a \u0437\u0430\u043a\u043b\u044e\u0447\u0435\u043d\u0438\u044e, \u0447\u0442\u043e \u043a\u0430\u043c\u0435\u0440\u0430 iPhone 5 \u044f\u0432\u043b\u044f\u0435\u0442\u0441\u044f \u043b\u0443\u0447\u0448\u0435\u0439 \u0441\u0440\u0435\u0434\u0438 \u0441\u043c\u0430\u0440\u0442\u0444\u043e\u043d\u043e\u0432 \u043e\u0442 \u0432\u0435\u0434\u0443\u0449\u0438\u0445 \u043f\u0440\u043e\u0438\u0437\u0432\u043e\u0434\u0438\u0442\u0435\u043b\u0435\u0439.\u000d\u000a\u000d\u000a\u000d\u000a\u000d\u000a\u0415\u0441\u043b\u0438 \u043f\u043e\u043f\u044b\u0442\u0430\u0442\u044c\u0441\u044f \u0440\u0430\u0441\u0441\u0442\u0430\u0432\u0438\u0442\u044c \u043f\u0440\u043e\u0442\u0435\u0441\u0442\u0438\u0440\u043e\u0432\u0430\u043d\u043d\u044b\u0435 \u0442\u0435\u043b\u0435\u0444\u043e\u043d\u044b \u043f\u043e \u043c\u0435\u0441\u0442\u0430\u043c \u0432 \u0437\u0430\u0432\u0438\u0441\u0438\u043c\u043e\u0441\u0442\u0438 \u043e\u0442 \u043f\u043e\u043b\u0443\u0447\u0435\u043d\u043d\u044b\u0445 \u0440\u0435\u0437\u0443\u043b\u044c\u0442\u0430\u0442\u043e\u0432, \u0442\u043e \u0432 \u0446\u0435\u043b\u043e\u043c \u043b\u0438\u0434\u0435\u0440\u043e\u043c \u0441\u043b\u0435\u0434\u0443\u0435\u0442 \u043f\u0440\u0438\u0437\u043d\u0430\u0442\u044c \u000d\u000aiPhone 5, \u0433\u043e\u0432\u043e\u0440\u044f\u0442 \u044d\u043a\u0441\u043f\u0435\u0440\u0442\u044b. \u041d\u0435\u0438\u0437\u0432\u0435\u0441\u0442\u043d\u043e, \u043a\u0430\u043a \u0430\u0432\u0442\u043e\u043c\u0430\u0442\u0438\u043a\u0430 \u0441\u043f\u0440\u0430\u0432\u043b\u044f\u0435\u0442\u0441\u044f \u043f\u0440\u0438 \u043c\u0438\u043d\u0438\u043c\u0430\u043b\u044c\u043d\u043e\u043c \u0443\u0447\u0430\u0441\u0442\u0438\u0438 \u0432\u043b\u0430\u0434\u0435\u043b\u044c\u0446\u0430, \u043d\u043e \u0442\u0435\u043b\u0435\u0444\u043e\u043d \u043e\u0442 Apple \u0432\u044b\u0434\u0430\u0435\u0442 \u0445\u043e\u0440\u043e\u0448\u0435\u0435 \u000d\u000a\u043a\u0430\u0447\u0435\u0441\u0442\u0432\u043e \u0441\u0442\u0430\u0431\u0438\u043b\u044c\u043d\u0435\u0435, \u0447\u0435\u043c \u043a\u043e\u043d\u043a\u0443\u0440\u0435\u043d\u0442\u044b, \u043f\u0440\u0438\u0447\u0435\u043c \u0432 \u0441\u0430\u043c\u044b\u0445 \u0440\u0430\u0437\u043d\u044b\u0445 \u0441\u0438\u0442\u0443\u0430\u0446\u0438\u044f\u0445.\u000d\u000a\u000d\u000a\u042d\u043a\u0441\u043f\u0435\u0440\u0442\u044b \u043f\u0440\u0438\u0448\u043b\u0438 \u043a \u0432\u044b\u0432\u043e\u0434\u0443 \u043f\u043e\u0441\u043b\u0435 \u0430\u043d\u0430\u043b\u0438\u0437\u0430 \u0442\u0435\u0441\u0442\u043e\u0432\u044b\u0445 \u0441\u043d\u0438\u043c\u043a\u043e\u0432, \u0447\u0442\u043e \u0441\u0438\u0441\u0442\u0435\u043c\u044b \u044d\u043a\u0441\u043f\u043e\u0437\u0430\u043c\u0435\u0440\u0430 \u0438 \u043e\u0446\u0435\u043d\u043a\u0438 \u0441\u044e\u0436\u0435\u0442\u0430 \u0432 \u0441\u043c\u0430\u0440\u0442\u0444\u043e\u043d\u0430\u0445 \u0440\u0430\u0431\u043e\u0442\u0430\u044e\u0442 \u043c\u0435\u043d\u0435\u0435 \u0443\u0441\u0442\u043e\u0439\u0447\u0438\u0432\u043e, \u0447\u0435\u043c \u000d\u000a\u0432 \u043d\u0430\u0441\u0442\u043e\u044f\u0449\u0438\u0445 \u0444\u043e\u0442\u043e\u043a\u0430\u043c\u0435\u0440\u0430\u0445. \u0412 \u043e\u0434\u043d\u0438\u0445 \u0438 \u0442\u0435\u0445 \u0436\u0435 \u0443\u0441\u043b\u043e\u0432\u0438\u044f\u0445, \u043f\u0440\u0438 \u0435\u0434\u0432\u0430 \u0437\u0430\u043c\u0435\u0442\u043d\u043e\u043c \u0438\u0437\u043c\u0435\u043d\u0435\u043d\u0438\u0438 \u043f\u043e\u043b\u043e\u0436\u0435\u043d\u0438\u044f \u0434\u0435\u0432\u0430\u0439\u0441\u0430, \u0430\u0432\u0442\u043e\u043c\u0430\u0442\u0438\u043a\u0430 \u043c\u043e\u0436\u0435\u0442 \u0441\u0440\u0435\u0430\u0433\u0438\u0440\u043e\u0432\u0430\u0442\u044c \u0441\u043e\u0432\u0441\u0435\u043c \u043f\u043e-\u000d\u000a\u0434\u0440\u0443\u0433\u043e\u043c\u0443, \u0432\u044b\u0441\u0442\u0430\u0432\u0438\u0432 \u0441\u0438\u043b\u044c\u043d\u043e \u043e\u0442\u043b\u0438\u0447\u0430\u044e\u0449\u0438\u0435\u0441\u044f \u043d\u0430\u0441\u0442\u0440\u043e\u0439\u043a\u0438. \u041f\u043e\u044d\u0442\u043e\u043c\u0443 \u043e\u043d\u0438 \u0440\u0435\u043a\u043e\u043c\u0435\u043d\u0434\u0443\u044e\u0442 \u0434\u0435\u043b\u0430\u0442\u044c \u043d\u0435\u0441\u043a\u043e\u043b\u044c\u043a\u043e \u0434\u0443\u0431\u043b\u0435\u0439, \u0447\u0442\u043e\u0431\u044b \u0445\u043e\u0442\u044f \u0431\u044b \u043e\u0434\u0438\u043d \u043e\u043a\u0430\u0437\u0430\u043b\u0441\u044f \u0443\u0434\u0430\u0447\u043d\u044b\u043c.\u000d\u000a\u000d\u000a\u000d\u000a\u000d\u000a\u0418\u0437\u0434\u0430\u043d\u0438\u0435 \u043e\u0442\u043c\u0435\u0447\u0430\u0435\u0442 \u043c\u0430\u043a\u0441\u0438\u043c\u0430\u043b\u044c\u043d\u0443\u044e \u043f\u0440\u043e\u0441\u0442\u043e\u0442\u0443 \u0438\u043d\u0442\u0435\u0440\u0444\u0435\u0439\u0441\u0430 \u0441\u044a\u0435\u043c\u043a\u0438 \u043f\u0440\u0438 \u043f\u043e\u043c\u043e\u0449\u0438 iPhone 5 \u0438 \u043b\u0443\u0447\u0448\u0438\u0439 \u0430\u043b\u0433\u043e\u0440\u0438\u0442\u043c \u0440\u0430\u0431\u043e\u0442\u044b \u0444\u0443\u043d\u043a\u0446\u0438\u0438 \u0440\u0435\u0436\u0438\u043c\u0430 HDR. \u041f\u0440\u0438 \u044d\u0442\u043e\u043c \u000d\u000a\u044d\u043a\u0441\u043f\u0435\u0440\u0442\u044b \u043d\u0430\u043f\u043e\u043c\u0438\u043d\u0430\u044e\u0442 \u0441\u043b\u0435\u0434\u0438\u0442\u044c \u0437\u0430 \u0447\u0438\u0441\u0442\u043e\u0442\u043e\u0439 \u0437\u0440\u0430\u0447\u043a\u0430 \u043a\u0430\u043c\u0435\u0440\u044b. \u041e\u0441\u043e\u0431\u0435\u043d\u043d\u043e \u043a\u0440\u0438\u0442\u0438\u0447\u043d\u044b\u043c \u043d\u0435\u0437\u0430\u043c\u0435\u0447\u0435\u043d\u043d\u044b\u0439 \u043e\u0442\u043f\u0435\u0447\u0430\u0442\u043e\u043a \u043f\u0430\u043b\u044c\u0446\u0430 \u043d\u0430 \u0441\u0442\u0435\u043a\u043b\u0435 \u043f\u0435\u0440\u0435\u0434 \u043a\u0430\u043c\u0435\u0440\u043e\u0439 \u0441\u0442\u0430\u043d\u0435\u0442 \u043f\u0440\u0438 \u000d\u000a\u0441\u044a\u0435\u043c\u043a\u0435 \u0441\u043e \u0432\u0441\u0442\u0440\u0435\u0447\u043d\u044b\u043c \u0441\u0432\u0435\u0442\u043e\u043c.\u000d\u000a\u000d\u000a\u041f\u0440\u0438\u0441\u043e\u0435\u0434\u0438\u043d\u044f\u0439\u0442\u0435\u0441\u044c \u043a \u043d\u0430\u043c \u0432 Twitter, \u0412\u041a\u043e\u043d\u0442\u0430\u043a\u0442\u0435, Facebook, Google+ \u0438\u043b\u0438 \u0447\u0435\u0440\u0435\u0437 RSS, \u0447\u0442\u043e\u0431\u044b \u0431\u044b\u0442\u044c \u0432 \u043a\u0443\u0440\u0441\u0435 \u043f\u043e\u0441\u043b\u0435\u0434\u043d\u0438\u0445 \u043d\u043e\u0432\u043e\u0441\u0442\u0435\u0439 \u0438\u0437 \u043c\u0438\u0440\u0430 Apple, Microsoft \u0438 \u000d\u000aGoogle.','\u042d\u043a\u0441\u043f\u0435\u0440\u0442\u044b \u043d\u0430\u0437\u0432\u0430\u043b\u0438 \u043a\u0430\u043c\u0435\u0440\u0443 iPhone 5 \u00ab\u043b\u0443\u0447\u0448\u0435\u0439 \u0441\u0440\u0435\u0434\u0438 \u0441\u0438\u043b\u044c\u043d\u0435\u0439\u0448\u0438\u0445\u00bb')
INSERT INTO NEWS VALUES(4,'2013-05-16 01:21:16.000000000','images/news4/Galaxy-S-III-3.jpg','images/news4/scaledGalaxy-S-III-3.jpg','\u0423\u0436\u0435 \u043f\u0435\u0440\u0435\u0434 \u043f\u043e\u043a\u0443\u043f\u043a\u043e\u0439 \u043d\u043e\u0432\u043e\u0433\u043e \u0441\u043c\u0430\u0440\u0442\u0444\u043e\u043d\u0430 \u0441\u0442\u043e\u0438\u0442 \u0437\u0430\u0434\u0443\u043c\u0430\u0442\u044c\u0441\u044f \u043e \u0435\u0433\u043e \u043f\u0440\u043e\u0434\u0430\u0436\u0435. \u041f\u0443\u0441\u0442\u044c \u0432\u044b \u0431\u0443\u0434\u0435\u0442\u0435 \u0440\u0430\u0441\u0441\u0442\u0430\u0432\u0430\u0442\u044c\u0441\u044f \u0441 \u043d\u0438\u043c \u043b\u0438\u0448\u044c \u0447\u0435\u0440\u0435\u0437 \u0433\u043e\u0434 \u0438\u043b\u0438 \u0434\u0432\u0430, \u043d\u043e \u0438\u0437\u0431\u0435\u0436\u0430\u0442\u044c \u000d\u000a\u043b\u0438\u0448\u043d\u0438\u0445 \u0444\u0438\u043d\u0430\u043d\u0441\u043e\u0432\u044b\u0445 \u043f\u043e\u0442\u0435\u0440\u044c \u0432\u044b \u043c\u043e\u0436\u0435\u0442\u0435 \u0443\u0436\u0435 \u0441\u0435\u0439\u0447\u0430\u0441. \u0414\u043b\u044f \u044d\u0442\u043e\u0433\u043e \u0434\u043e\u0441\u0442\u0430\u0442\u043e\u0447\u043d\u043e \u0437\u043d\u0430\u0442\u044c, \u043d\u0430\u0441\u043a\u043e\u043b\u044c\u043a\u043e \u0431\u044b\u0441\u0442\u0440\u043e \u0442\u0435\u0440\u044f\u0435\u0442 \u0432 \u0446\u0435\u043d\u0435 \u0442\u0430 \u0438\u043b\u0438 \u0438\u043d\u0430\u044f \u043c\u043e\u0434\u0435\u043b\u044c.\u000d\u000a\u000d\u000a\u000d\u000a\u000d\u000a\u0418\u043d\u0432\u0435\u0441\u0442\u0438\u0446\u0438\u043e\u043d\u043d\u044b\u0439 \u0431\u0430\u043d\u043a Piper Jaffray \u043e\u0431\u043d\u0430\u0440\u043e\u0434\u043e\u0432\u0430\u043b \u0440\u0435\u0437\u0443\u043b\u044c\u0442\u0430\u0442\u044b \u0438\u0441\u0441\u043b\u0435\u0434\u043e\u0432\u0430\u043d\u0438\u044f \u0432\u0442\u043e\u0440\u0438\u0447\u043d\u043e\u0433\u043e \u0440\u044b\u043d\u043a\u0430 \u0441\u043c\u0430\u0440\u0442\u0444\u043e\u043d\u043e\u0432, \u0441\u0442\u043e\u0438\u043c\u043e\u0441\u0442\u044c \u043a\u043e\u0442\u043e\u0440\u044b\u0445 \u043d\u0435 \u0432\u0441\u0435\u0433\u0434\u0430 \u0437\u0430\u0432\u0438\u0441\u0438\u0442 \u043e\u0442 \u000d\u000a\u0441\u043e\u0441\u0442\u043e\u044f\u043d\u0438\u044f \u0438 \u0438\u0437\u043d\u043e\u0441\u0430 \u0443\u0441\u0442\u0440\u043e\u0439\u0441\u0442\u0432\u0430. \u0421\u043e\u0433\u043b\u0430\u0441\u043d\u043e \u043e\u0442\u0447\u0435\u0442\u0443 \u043a\u043e\u043c\u043f\u0430\u043d\u0438\u0438, \u0442\u0435\u043b\u0435\u0444\u043e\u043d\u044b Apple \u0434\u043e\u043b\u044c\u0448\u0435 \u0441\u043e\u0445\u0440\u0430\u043d\u044f\u044e\u0442 \u0446\u0435\u043d\u0443, \u0447\u0435\u043c \u0442\u043e\u043f\u043e\u0432\u044b\u0435 \u043a\u043e\u043c\u043c\u0443\u043d\u0438\u043a\u0430\u0442\u043e\u0440\u044b \u043a\u043e\u043d\u043a\u0443\u0440\u0438\u0440\u0443\u044e\u0449\u0435\u0439 \u000d\u000aSamsung.\u000d\u000a\u000d\u000a\u0421\u0442\u0430\u0442\u0438\u0441\u0442\u0438\u043a\u0430 \u043d\u0430\u043a\u0430\u043f\u043b\u0438\u0432\u0430\u043b\u0430\u0441\u044c \u043c\u0435\u0442\u043e\u0434\u043e\u043c \u0438\u0437\u0443\u0447\u0435\u043d\u0438\u044f \u0440\u0435\u0437\u0443\u043b\u044c\u0442\u0430\u0442\u043e\u0432 \u043c\u043d\u043e\u0433\u043e\u0447\u0438\u0441\u043b\u0435\u043d\u043d\u044b\u0445 \u0441\u0434\u0435\u043b\u043e\u043a \u043d\u0430 \u0441\u0430\u0439\u0442\u0435 eBay. \u0412 \u0438\u0441\u0441\u043b\u0435\u0434\u043e\u0432\u0430\u043d\u0438\u0438 \u043f\u0440\u0438\u043d\u0438\u043c\u0430\u043b\u0438 \u0443\u0447\u0430\u0441\u0442\u0438\u0435 \u0442\u0440\u0438 \u0441\u043c\u0430\u0440\u0442\u0444\u043e\u043d\u0430 \u000d\u000aApple \u2013 iPhone 5, iPhone 4, iPhone 4S \u2013 \u0438 \u0434\u0432\u0430 \u0443\u0441\u0442\u0440\u043e\u0439\u0441\u0442\u0432\u0430 Samsung \u2013 Galaxy S III \u0438 Galaxy Note II. \u0412\u044b\u044f\u0441\u043d\u0438\u043b\u043e\u0441\u044c, \u0447\u0442\u043e \u043f\u043e\u0442\u0435\u0440\u044f \u0441\u0442\u043e\u0438\u043c\u043e\u0441\u0442\u0438 \u043f\u0440\u043e\u0438\u0441\u0445\u043e\u0434\u0438\u0442 \u0443 \u0440\u0430\u0437\u043d\u044b\u0445 \u000d\u000a\u043c\u043e\u0434\u0435\u043b\u0435\u0439 \u0442\u0435\u043b\u0435\u0444\u043e\u043d\u043e\u0432 \u043f\u043e-\u0440\u0430\u0437\u043d\u043e\u043c\u0443.\u000d\u000a\u000d\u000a\u000d\u000a\u000d\u000a\u041a \u043f\u0440\u0438\u043c\u0435\u0440\u0443, \u0432\u043b\u0430\u0434\u0435\u043b\u044c\u0446\u044b \u043f\u043b\u0430\u043d\u0448\u0435\u0442\u043e\u0444\u043e\u043d\u0430 Samsung Galaxy Note II \u043f\u043e\u0442\u0435\u0440\u044f\u043b\u0438 \u0431\u043e\u043b\u044c\u0448\u0443\u044e \u0441\u0443\u043c\u043c\u0443 \u043f\u0440\u0438 \u043f\u0435\u0440\u0435\u043f\u0440\u043e\u0434\u0430\u0436\u0435, \u0447\u0435\u043c \u0432\u043b\u0430\u0434\u0435\u043b\u044c\u0446\u044b Galaxy S III. \u041f\u0440\u0438 \u044d\u0442\u043e\u043c \u043c\u0435\u043d\u044c\u0448\u0435 \u000d\u000a\u0432\u0441\u0435\u0433\u043e \u043f\u043e\u0434\u0432\u0435\u0440\u0436\u0435\u043d\u044b \u043e\u0431\u0435\u0441\u0446\u0435\u043d\u0438\u0432\u0430\u043d\u0438\u044e \u043c\u043e\u0434\u0435\u043b\u0438 \u00ab\u044f\u0431\u043b\u043e\u0447\u043d\u044b\u0445\u00bb \u0441\u043c\u0430\u0440\u0442\u0444\u043e\u043d\u043e\u0432. \u0417\u0430 \u0434\u0432\u0430 \u043c\u0435\u0441\u044f\u0446\u0430, \u0432 \u0442\u0435\u0447\u0435\u043d\u0438\u0435 \u043a\u043e\u0442\u043e\u0440\u044b\u0445 \u0444\u0438\u043a\u0441\u0438\u0440\u043e\u0432\u0430\u043b\u0438\u0441\u044c \u0441\u0434\u0435\u043b\u043a\u0438 \u043d\u0430 eBay, Galaxy Note II \u000d\u000a\u043f\u043e\u0442\u0435\u0440\u044f\u043b \u0432 \u0446\u0435\u043d\u0435 15%, \u043a\u043e\u0433\u0434\u0430 iPhone 4S \u2013 \u043b\u0438\u0448\u044c 7%. \u041c\u0435\u0436\u0434\u0443 \u043d\u0438\u043c\u0438 \u0432 \u043f\u043e\u0440\u044f\u0434\u043a\u0435 \u0443\u0431\u044b\u0432\u0430\u043d\u0438\u044f \u0446\u0435\u043d \u043d\u0430 \u0432\u0442\u043e\u0440\u0438\u0447\u043d\u043e\u043c \u0440\u044b\u043d\u043a\u0435 \u2013 Galaxy S III, iPhone 5 \u0438 iPhone 4 \u0441 \u000d\u000a\u043f\u043e\u043a\u0430\u0437\u0430\u0442\u0435\u043b\u044f\u043c\u0438 13,7%, 11,2% \u0438 8,8% \u0441\u043e\u043e\u0442\u0432\u0435\u0442\u0441\u0442\u0432\u0435\u043d\u043d\u043e.\u000d\u000a\u000d\u000a\u000d\u000a\u000d\u000a\u0421\u043e\u0433\u043b\u0430\u0441\u043d\u043e \u0440\u0435\u0437\u0443\u043b\u044c\u0442\u0430\u0442\u0430\u043c \u0438\u0441\u0441\u043b\u0435\u0434\u043e\u0432\u0430\u043d\u0438\u044f, \u0437\u0430 \u0434\u0432\u0430 \u043c\u0435\u0441\u044f\u0446\u0430 \u0441\u0442\u043e\u0438\u043c\u043e\u0441\u0442\u044c iPhone 4S \u2013 \u0440\u0435\u043a\u043e\u0440\u0434\u0441\u043c\u0435\u043d\u0430 \u043f\u043e \u0443\u0434\u0435\u0440\u0436\u0430\u043d\u0438\u044e \u0446\u0435\u043d\u044b \u2013 \u0441\u043d\u0438\u0437\u0438\u043b\u0430\u0441\u044c \u0441 $349 \u0432 \u043c\u0430\u0440\u0442\u0435 \u0434\u043e $312 \u0432 \u043c\u0430\u0435 \u000d\u000a\u044d\u0442\u043e\u0433\u043e \u0433\u043e\u0434\u0430, \u0430 \u0446\u0435\u043d\u0430 iPhone 5 \u0443\u043f\u0430\u043b\u0430 \u0441 $535 \u0434\u043e $465 \u0437\u0430 \u0442\u043e\u0442 \u0436\u0435 \u043f\u0435\u0440\u0438\u043e\u0434. \u0427\u0442\u043e \u043a\u0430\u0441\u0430\u0435\u0442\u0441\u044f \u043a\u043e\u0440\u0435\u0439\u0441\u043a\u0438\u0445 \u0430\u043f\u043f\u0430\u0440\u0430\u0442\u043e\u0432, \u0442\u043e \u0438\u0445 \u0446\u0435\u043d\u0430 \u0441\u043d\u0438\u0437\u0438\u043b\u0430\u0441\u044c \u0434\u043e $320 \u0441 $360 \u0432 \u0441\u043b\u0443\u0447\u0430\u0435 \u000d\u000aGalaxy S III \u0438 \u0434\u043e $444 \u0441 $550 \u0432 \u0441\u043b\u0443\u0447\u0430\u0435 \u0441 Galaxy Note II.\u000d\u000a\u000d\u000a\u041f\u0440\u0438\u0441\u043e\u0435\u0434\u0438\u043d\u044f\u0439\u0442\u0435\u0441\u044c \u043a \u043d\u0430\u043c \u0432 Twitter, \u0412\u041a\u043e\u043d\u0442\u0430\u043a\u0442\u0435, Facebook, Google+ \u0438\u043b\u0438 \u0447\u0435\u0440\u0435\u0437 RSS, \u0447\u0442\u043e\u0431\u044b \u0431\u044b\u0442\u044c \u0432 \u043a\u0443\u0440\u0441\u0435 \u043f\u043e\u0441\u043b\u0435\u0434\u043d\u0438\u0445 \u043d\u043e\u0432\u043e\u0441\u0442\u0435\u0439 \u0438\u0437 \u043c\u0438\u0440\u0430 Apple, Microsoft \u0438 \u000d\u000aGoogle.\u000d\u000a\u000d\u000a\u0009 \u0416\u0430\u043b\u043a\u043e. \u041e\u0442\u043a\u0440\u043e\u0432\u0435\u043d\u043d\u043e \u0436\u0430\u043b\u043a\u043e "\u0411\u0435\u043d\u0444\u0438\u043a\u0443", \u043a\u043e\u0442\u043e\u0440\u0430\u044f \u0438\u0433\u0440\u0430\u043b\u0430 \u043b\u0443\u0447\u0448\u0435. \u0414\u043e\u043b\u0436\u043d\u0430 \u0431\u044b\u043b\u0430 \u0437\u0430\u0431\u0438\u0432\u0430\u0442\u044c \u0435\u0449\u0435 \u0432 \u043f\u0435\u0440\u0432\u043e\u043c \u0442\u0430\u0439\u043c\u0435, \u043d\u043e \u043d\u0435 \u0434\u043e\u0442\u0435\u0440\u043f\u0435\u043b\u0430 \u0432\u0441\u0435\u0433\u043e \u043b\u0438\u0448\u043b\u044c 40 \u0441\u0435\u043a\u0443\u043d\u0434 \u000d\u000a\u0434\u043e \u043e\u0432\u0435\u0440\u0442\u0430\u0439\u043c\u0430. \u0411\u0435\u043d\u0438\u0442\u0435\u0441 \u043d\u0435 \u0441\u0434\u0435\u043b\u0430\u043b \u043d\u0438 \u043e\u0434\u043d\u043e\u0439 \u0437\u0430\u043c\u0435\u043d\u044b, \u043d\u043e \u0444\u0438\u0437\u0438\u0447\u0435\u0441\u043a\u0438\u0445 \u0441\u0438\u043b \u043e\u0441\u043d\u043e\u0432\u043d\u044b\u043c \u0438\u0433\u0440\u043e\u043a\u0430\u043c \u043b\u043e\u043d\u0434\u043e\u043d\u0446\u0435\u0432 \u0445\u0432\u0430\u0442\u0438\u043b\u043e, \u0447\u0442\u043e\u0431\u044b \u043f\u043e\u0431\u0435\u0434\u0438\u0442\u044c. \u042d\u043f\u0438\u0447\u043d\u044b\u0439 \u0444\u0438\u043d\u0430\u043b! \u000d\u000a\u041f\u0440\u043e\u0441\u0442\u043e \u044d\u043f\u0438\u0447\u043d\u044b\u0439!\u000d\u000a\u0009 \u0412\u044b\u0431\u0438\u0432\u0430\u0435\u0442 \u043c\u044f\u0447 \u0427\u0435\u0445, \u0438 \u0437\u0432\u0443\u0447\u0438\u0442 \u0444\u0438\u043d\u0430\u043b\u044c\u043d\u044b\u0439 \u0441\u0432\u0438\u0441\u0442\u043e\u043a. "\u0427\u0435\u043b\u0441\u0438" \u0432\u044b\u0438\u0433\u0440\u044b\u0432\u0430\u0435\u0442 \u0432\u0442\u043e\u0440\u043e\u0439 \u0435\u0432\u0440\u043e\u043f\u0435\u0439\u0441\u043a\u0438\u0439 \u0442\u0440\u043e\u0444\u0435\u0439 \u043f\u043e\u0434\u0440\u044f\u0434, \u0411\u0435\u043d\u0438\u0442\u0435\u0441 \u0443\u0445\u043e\u0434\u0438\u0442 \u043f\u043e\u0431\u0435\u0434\u0438\u0442\u0435\u043b\u0435\u043c.\u000d\u000a+4\u0009\u0009 !!! \u042d\u0442\u043e \u043f\u0440\u043e\u0441\u0442\u043e \u0441\u0443\u043f\u0435\u0440\u0438\u0433\u0440\u0430! \u0427\u0443\u0442\u044c \u043d\u0435 \u0437\u0430\u0431\u0438\u043b\u0438 "\u043e\u0440\u043b\u044b"! \u0417\u0430\u0431\u0440\u043e\u0441 \u0432 \u0448\u0442\u0440\u0430\u0444\u043d\u0443\u044e \u043f\u043e\u0448\u0435\u043b \u043d\u0430 \u041a\u0430\u0440\u0434\u043e\u0441\u043e, \u0430 \u0442\u0430\u043c \u0418\u0432\u0430\u043d\u043e\u0432\u0438\u0447 \u0437\u0430\u043f\u0443\u0442\u0430\u043b\u0441\u044f \u0432 \u043d\u043e\u0433\u0430\u0445 \u0438 \u043d\u0435 \u0441\u043c\u043e\u0433 \u0432\u044b\u0431\u0438\u0442\u044c \u000d\u000a\u043c\u044f\u0447. \u041e\u0441\u043a\u0430\u0440\u0443 \u043e\u0441\u0442\u0430\u0432\u0430\u043b\u043e\u0441\u044c \u0442\u043e\u043b\u044c\u043a\u043e \u043f\u0440\u043e\u0442\u043e\u043b\u043a\u043d\u0443\u0442\u044c \u0441\u043d\u0430\u0440\u044f\u0434 \u0432 \u0432\u043e\u0440\u043e\u0442\u0430, \u043d\u043e \u0442\u0430\u043c \u0432\u044b\u0440\u043e\u0441 \u0447\u0430\u0441\u0442\u043e\u043a\u043e\u043b \u043d\u043e\u0433 \u0438 \u043d\u0435 \u0443\u0434\u0430\u043b\u043e\u0441\u044c \u044d\u0442\u043e \u0441\u0434\u0435\u043b\u0430\u0442\u044c, \u0430 \u044d\u0442\u043e \u0431\u044b\u043b\u0430 \u0431\u044b \u0441\u0443\u043f\u0435\u0440\u0440\u0430\u0437\u0432\u044f\u0437\u043a\u0430!\u000d\u000a+3\u0009\u0009 !!! \u041c\u043e\u0433\u043b\u0438 \u0432\u0441\u0435 \u0440\u0435\u0448\u0438\u0442\u044c! \u0418 \u0440\u0435\u0448\u0438\u043b\u0438! \u041d\u0430\u0432\u0435\u0441 \u0441 \u043f\u0440\u0430\u0432\u043e\u0433\u043e \u043a\u043e\u0440\u043d\u0435\u0440\u0430 \u0441\u043b\u0435\u0434\u0443\u0435\u0442 \u043e\u0442 \u041c\u0430\u0442\u044b \u0432 \u0446\u0435\u043d\u0442\u0440 \u0448\u0442\u0440\u0430\u0444\u043d\u043e\u0439, \u0433\u0434\u0435 \u043f\u043e\u0447\u0435\u043c\u0443-\u0442\u043e \u043d\u0438\u043a\u0442\u043e \u043d\u0435 \u043f\u0440\u044b\u0433\u0430\u043b \u0441 \u0418\u0432\u0430\u043d\u043e\u0432\u0438\u0447\u0435\u043c, \u000d\u000a\u043a\u043e\u0442\u043e\u0440\u044b\u0439 \u043f\u0435\u0440\u0435\u043f\u0440\u044b\u0433\u043d\u0443\u043b \u0437\u0430\u0449\u0438\u0442\u043d\u0438\u043a\u0430 \u0438 \u043f\u043e \u0434\u0443\u0433\u0435 \u043d\u0430\u043f\u0440\u0430\u0432\u0438\u043b \u043c\u044f\u0447 \u043f\u043e\u0434 \u043f\u0440\u0430\u0432\u0443\u044e \u0448\u0442\u0430\u043d\u0433\u0443 \u0447\u0443\u0442\u044c \u043d\u0438\u0436\u0435 "\u0434\u0435\u0432\u044f\u0442\u043a\u0438". \u0412 \u0446\u0435\u043d\u0442\u0440\u0435 \u0432\u0440\u0430\u0442\u0430\u0440\u0441\u043a\u043e\u0439 \u0437\u0430\u0441\u0442\u044b\u043b \u0410\u0440\u0442\u0443\u0440, \u043a\u043e\u0442\u043e\u0440\u044b\u0439 \u0443\u0436\u0435 \u000d\u000a\u043d\u0438\u0447\u0435\u043c \u043d\u0435 \u0441\u043c\u043e\u0433 \u043f\u043e\u043c\u043e\u0447\u044c \u0441\u0432\u043e\u0435\u0439 \u043a\u043e\u043c\u0430\u043d\u0434\u0435.\u000d\u000a+3\u0009\u0009 \u0413\u041e\u041e\u041e\u041e\u041e\u041b!!! \u0418\u0432\u0430\u043d\u043e\u0432\u0438\u0447, 1:2\u000d\u000a+2\u0009\u0009 \u0420\u0430\u043c\u0438\u0440\u0435\u0441 \u0432 \u043e\u0434\u043d\u043e\u0439 \u0438\u0437 \u043f\u043e\u0441\u043b\u0435\u0434\u043d\u0438\u0445 \u0430\u0442\u0430\u043a "\u0427\u0435\u043b\u0441\u0438" \u0437\u0430\u0440\u0430\u0431\u0430\u0442\u044b\u0432\u0430\u0435\u0442 \u0443\u0433\u043b\u0432\u043e\u043e\u0439 \u043f\u043e \u043f\u0440\u0430\u0432\u043e\u043c\u0443 \u043a\u0440\u0430\u044e. \u041c\u043e\u0436\u043d\u043e \u0432\u0441\u0435 \u0440\u0435\u0448\u0438\u0442\u044c!\u000d\u000a90\u0009\u0009 3 \u043c\u0438\u043d\u0443\u0442\u044b \u0434\u043e\u0431\u0430\u0432\u043b\u044f\u0435\u0442 \u041a\u0435\u0439\u043f\u0435\u0440\u0441. \u041c\u044b \u0432\u0441\u0435 \u0431\u043b\u0438\u0436\u0435 \u043a \u043e\u0432\u0435\u0440\u0442\u0430\u0439\u043c\u0443. \u041e\u043d \u0443\u0436\u0435 \u043f\u043e\u0447\u0442\u0438 \u043d\u0435\u043c\u0438\u043d\u0443\u0435\u043c.\u000d\u000a90\u0009\u0009 \u041f\u043e\u0441\u043b\u0435 \u0432\u0431\u0440\u043e\u0441\u0430 \u0430\u0443\u0442\u0430 \u0441 \u043f\u0440\u0430\u0432\u043e\u0433\u043e \u0444\u043b\u0430\u043d\u0433\u0430 \u0421\u0430\u043b\u044c\u0432\u0438\u043e \u043d\u0430\u0432\u0435\u0441\u0438\u043b \u0432 \u0446\u0435\u043d\u0442\u0440 \u0448\u0442\u0440\u0430\u0444\u043d\u043e\u0439. \u041b\u0443\u0438\u0437\u0430\u043e \u0433\u043e\u043b\u043e\u0432\u043e\u0439 \u0441\u0431\u0440\u043e\u0441\u0438\u043b \u0441\u043d\u0430\u0440\u044f\u0434 \u043d\u0430 \u043b\u0438\u043d\u0438\u044e \u0432\u0440\u0430\u0442\u0430\u0440\u0441\u043a\u043e\u0439, \u043d\u043e \u043f\u0440\u043e\u0431\u0438\u0442\u044c \u000d\u000a\u0442\u0430\u043c \u0431\u044b\u043b\u043e \u043d\u0435\u043a\u043e\u043c\u0443. \u0412\u044b\u043d\u043e\u0441.\u000d\u000a89\u0009\u0009 \u0414\u0432\u0443\u043c\u044f \u0430\u0442\u0430\u043a\u0430\u043c\u0438 \u043f\u043e\u0434\u0440\u044f\u0434 \u043e\u0442\u0432\u0435\u0447\u0430\u0435\u0442 "\u0411\u0435\u043d\u0444\u0438\u043a\u0430". \u0421\u043d\u0430\u0447\u0430\u043b\u0430 \u041b\u0438\u043c\u0430 \u0441 \u043b\u0435\u0432\u043e\u0433\u043e \u0443\u0433\u043b\u0430 \u0448\u0442\u0440\u0430\u0444\u043d\u043e\u0439 \u043f\u0430\u043b\u044c\u043d\u0443\u043b \u0441\u0438\u043b\u044c\u043d\u043e \u0432\u044b\u0448\u0435 \u0446\u0435\u043b\u0438. \u0410 \u0432\u043e\u0442 \u0432\u043e \u0432\u0442\u043e\u0440\u043e\u0439 \u0430\u0442\u0430\u043a\u0435 \u0410\u043b\u043c\u0435\u0439\u0434\u0430 \u000d\u000a\u0441 \u043f\u0440\u0430\u0432\u043e\u0433\u043e \u0444\u043b\u0430\u043d\u0433\u0430 \u0437\u0430\u0440\u044f\u0434\u0438\u043b \u0432 \u0448\u0442\u0440\u0430\u0444\u043d\u0443\u044e, \u0433\u0434\u0435 \u0431\u043e\u0440\u043e\u043b\u0438\u0441\u044c \u0432\u0441\u0435 \u0437\u0430 \u043c\u044f\u0447, \u043d\u043e "\u0441\u0438\u043d\u0438\u0435" \u0432\u0441\u0435-\u0442\u0430\u043a\u0438 \u0432\u044b\u0431\u0438\u043b\u0438 \u0441\u0444\u0435\u0440\u0443 \u0437\u0430 \u0431\u0440\u043e\u0432\u043a\u0443.\u000d\u000a88\u0009\u0009 !!! \u0412\u043e\u0442 \u044d\u0442\u043e \u0432\u044b\u0441\u0442\u0440\u0435\u043b! \u041c\u0430\u0442\u0430 \u043f\u043e\u0431\u043e\u0440\u043e\u043b\u0441\u044f \u043d\u0430 \u0447\u0443\u0436\u043e\u0439 \u043f\u043e\u043b\u043e\u0432\u0438\u043d\u0435 \u043f\u043e\u043b\u044f \u0438 \u0441\u043c\u043e\u0433 \u043f\u0440\u043e\u0442\u043e\u043b\u043a\u043d\u0443\u0442\u044c \u043c\u044f\u0447 \u0432 \u0446\u0435\u043d\u0442\u0440 \u043d\u0430 \u041b\u044d\u043c\u043f\u0430\u0440\u0434\u0430. \u041d\u0438\u043a\u0442\u043e \u043d\u0435 \u043d\u0430\u043a\u0440\u044b\u043b \u0424\u0440\u044d\u043d\u043a\u0430, \u043a\u043e\u0442\u043e\u0440\u044b\u0439 \u000d\u000a\u043f\u043e\u0434\u0440\u0430\u0431\u043e\u0442\u0430\u043b \u043f\u043e\u0434 \u043f\u0440\u0430\u0432\u0443\u044e \u0438 \u0441 25 \u043c\u0435\u0442\u0440\u043e\u0432 \u0441\u0430\u0434\u0430\u043d\u0443\u043b \u0432 \u043f\u0440\u0430\u0432\u0443\u044e "\u0434\u0435\u0432\u044f\u0442\u043a\u0443". \u0422\u043e\u043b\u044c\u043a\u043e \u043f\u0435\u0440\u0435\u043a\u043b\u0430\u0434\u0438\u043d\u0430 \u0441\u043f\u0430\u0441\u0430\u0435\u0442 \u0432\u043e\u0440\u043e\u0442\u0430 \u0410\u0440\u0442\u0443\u0440\u0430!\u000d\u000a88\u0009\u0009 \u0413\u0430\u0439\u0442\u0430\u043d \u0441\u043e \u0441\u0442\u0430\u043d\u0434\u0430\u0440\u0442\u0430 \u0441 \u043b\u0435\u0432\u043e\u0433\u043e \u043a\u0440\u0430\u044f \u043d\u0430\u0432\u0435\u0441\u0438\u043b \u0432 \u0448\u0442\u0440\u0430\u0444\u043d\u0443\u044e, \u0433\u0434\u0435 \u041c\u0430\u0442\u0438\u0447 \u0433\u043e\u043b\u043e\u0432\u043e\u0439 \u043f\u0440\u043e\u0431\u0438\u0432\u0430\u043b \u043f\u043e \u0432\u044b\u0441\u043e\u043a\u043e\u0439 \u0434\u0443\u0433\u0435 \u043d\u0430 \u043f\u0440\u0430\u0432\u0443\u044e \u0448\u0442\u0430\u043d\u0433\u0443, \u043d\u043e \u0442\u0430\u043c \u0427\u0435\u0445 \u000d\u000a\u0432\u044b\u0438\u0433\u0440\u044b\u0432\u0430\u0435\u0442 \u0432\u043e\u0437\u0434\u0443\u0445.\u000d\u000a87\u0009\u0009 \u0418\u0433\u0440\u0430 \u0438\u0434\u0435\u0442 \u043f\u0440\u0430\u043a\u0442\u0438\u0447\u0435\u0441\u043a\u0438 \u0431\u0435\u0437 \u0446\u0435\u043d\u0442\u0440\u0430 \u043f\u043e\u043b\u044f. \u0422\u0435\u043f\u0435\u0440\u044c \u0443\u0436\u0435 \u0448\u0442\u0440\u0430\u0444\u043d\u043e\u0439 \u043d\u0430 \u0447\u0443\u0436\u043e\u0439 \u043f\u043e\u043b\u043e\u0432\u0438\u043d\u0435 \u0437\u0430\u0440\u0430\u0431\u0430\u0442\u044b\u0432\u0430\u044e\u0442 \u043f\u043e\u0440\u0442\u0443\u0433\u0430\u043b\u044c\u0446\u044b.\u000d\u000a87\u0009\u0009 \u0410\u0441\u043f\u0438\u043b\u0438\u043a\u0443\u044d\u0442\u0430 \u0432 \u043a\u043e\u043d\u0446\u0435 \u0430\u0442\u0430\u043a\u0438 "\u0427\u0435\u043b\u0441\u0438" \u043f\u0440\u043e\u0431\u0438\u0432\u0430\u0435\u0442 \u0441 \u043f\u0440\u0430\u0432\u043e\u0433\u043e \u043a\u0440\u0430\u044f. \u041d\u0435 \u0441\u0438\u043b\u044c\u043d\u043e, \u043d\u043e \u0442\u043e\u0447\u043d\u043e. \u0420\u0430\u0437\u043c\u044f\u043b \u0410\u0440\u0442\u0443\u0440\u0430, \u043a\u043e\u0442\u043e\u0440\u044b\u0439 \u0432 \u043f\u0430\u0434\u0435\u043d\u0438\u0438 \u043d\u0430\u043a\u0440\u044b\u043b \u0441\u043d\u0430\u0440\u044f\u0434.\u000d\u000a85\u0009\u0009 \u0414\u0430 \u0447\u0442\u043e \u0436 \u0442\u0430\u043a\u043e\u0435... \u042d\u043d\u0446\u043e \u041f\u0435\u0440\u0435\u0441 \u0432 \u0431\u043e\u0440\u044c\u0431\u0435 \u0441 \u0414\u0430\u0432\u0438\u0434\u043e\u043c \u041b\u0443\u0438\u0441\u043e\u043c \u043f\u043e\u043a\u0430\u0442\u0438\u043b\u0441\u044f \u0432 \u043f\u043e\u0434\u043a\u0430\u0442\u0435 \u0438 \u0434\u0435\u0440\u043d\u0443\u043b \u043a\u0430\u043a\u0443\u044e-\u0442\u043e \u043c\u044b\u0448\u0446\u0443. \u0418\u043b\u0438 \u043f\u0440\u043e\u0441\u0442\u043e \u0441\u0438\u043b\u044c\u043d\u043e \u0443\u0434\u0430\u0440\u0438\u043b. \u041d\u043e \u000d\u000a\u043d\u0438\u0447\u0435\u0433\u043e. \u0414\u0430\u0436\u0435 \u0432\u0440\u0430\u0447\u0435\u0439 \u043d\u0435 \u043f\u043e\u043d\u0430\u0434\u043e\u0431\u0438\u043b\u043e\u0441\u044c.\u000d\u000a84\u0009\u0009 \u0425\u043e\u0440\u043e\u0448! \u0425\u043e\u0440\u043e\u0448 \u0420\u0430\u043c\u0438\u0440\u0435\u0441, \u043d\u043e \u043d\u0435 \u043c\u0435\u043d\u0435\u0435 \u0445\u043e\u0440\u043e\u0448 \u0438 \u041b\u0443\u0438\u0437\u0430\u043e! \u0412\u043e\u0441\u043f\u043e\u043b\u044c\u0437\u043e\u0432\u0430\u043b\u0441\u044f \u043d\u0435\u0440\u0430\u0437\u0431\u0435\u0440\u0438\u0445\u043e\u0439 \u0445\u0430\u0432\u0431\u0435\u043a "\u0427\u0435\u043b\u0441\u0438", \u0443\u0431\u0435\u0433\u0430\u043b \u0441\u043f\u0440\u0430\u0432\u0430 \u043e\u0442 \u0446\u0435\u043d\u0442\u0440\u0430, \u0432\u043b\u0435\u0442\u0435\u043b \u0432 \u000d\u000a\u0448\u0442\u0440\u0430\u0444\u043d\u0443\u044e \u0438 \u043f\u0440\u043e\u0431\u0438\u0432\u0430\u043b \u043f\u043e\u0447\u0442\u0438 \u0441 \u0443\u0433\u043b\u0430 \u0432\u0440\u0430\u0442\u0430\u0440\u0441\u043a\u043e\u0439, \u043d\u043e \u0432\u043e\u0432\u0440\u0435\u043c\u044f \u043f\u0440\u044b\u0433\u043d\u0443\u043b \u0432 \u043f\u043e\u0434\u043a\u0430\u0442\u0435 \u041b\u0443\u0438\u0437\u0430\u043e, \u043a\u043e\u0442\u043e\u0440\u044b\u0439 \u043d\u043e\u0433\u0430\u043c\u0438 \u0438 \u0437\u0430\u0431\u043b\u043e\u043a\u0438\u0440\u043e\u0432\u0430\u043b \u0432\u044b\u0441\u0442\u0440\u0435\u043b. \u0423\u0433\u043b\u043e\u0432\u043e\u0439 \u043d\u0438\u0447\u0435\u0433\u043e \u043d\u0435 \u000d\u000a\u043f\u0440\u0438\u043d\u0435\u0441 \u0433\u043e\u0441\u0442\u044f\u043c.\u000d\u000a83\u0009\u0009 \u041d\u0430\u0432\u0435\u0441 \u0441 \u043f\u0440\u0430\u0432\u043e\u0433\u043e \u043a\u043e\u0440\u043d\u0435\u0440\u0430 \u0438\u0434\u0435\u0442 \u0432 \u0446\u0435\u043d\u0442\u0440 \u0448\u0442\u0440\u0430\u0444\u043d\u043e\u0439, \u0433\u0434\u0435 \u0433\u043e\u043b\u043e\u0432\u043e\u0439 \u043f\u0440\u043e\u0431\u0438\u0432\u0430\u0435\u0442 \u0416\u0430\u0440\u0434\u0435\u043b. \u0412\u044b\u0448\u0435 \u043f\u0435\u0440\u0435\u043a\u043b\u0430\u0434\u0438\u043d\u044b. \u041d\u043e \u0434\u0430\u044e\u0442 \u0431\u0438\u0442\u044c \u043b\u043e\u043d\u0434\u043e\u043d\u0446\u044b, \u044d\u0442\u043e \u043d\u0430\u0434\u043e \u000d\u000a\u043e\u0442\u043c\u0435\u0442\u0438\u0442\u044c.','\u0418\u0441\u0441\u043b\u0435\u0434\u043e\u0432\u0430\u043d\u0438\u0435: \u0442\u043e\u043f\u043e\u0432\u044b\u0435 \u0441\u043c\u0430\u0440\u0442\u0444\u043e\u043d\u044b Samsung \u0434\u0435\u0448\u0435\u0432\u0435\u044e\u0442 \u0431\u044b\u0441\u0442\u0440\u0435\u0435 iPhone')
